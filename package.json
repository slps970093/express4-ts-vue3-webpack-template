    "@vue/compiler-sfc": "^3.1.5",
    "body-parser": "^1.19.0",
    "concurrently": "^7.4.0",
    "cors": "^2.8.5",
    "dotenv": "^10.0.0",
    "dotenv-webpack": "^8.0.1",
    "express": "^4.17.1",
    "ts-loader": "^9.2.3",
    "ts-node": "^10.9.1",
    "ts-node-dev": "^2.0.0",
    "typescript": "^4.3.2",
    "vue-loader": "^16.1.2",
    "webpack": "^5.49.0",
    "webpack-cli": "^4.7.2",
