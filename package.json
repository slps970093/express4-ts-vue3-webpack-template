{
  "name": "ts-express",
  "version": "1.0.0",
  "description": "Simple Typescript Express with Webpack, Model, Controller. For Api purpose.",
  "main": "src/server.ts",
  "scripts": {
    "dev:express": "ts-node ./src/server.ts",
    "dev:webpack": "webpack --watch --mode=development",
